@namespace Skclusive.Material.Chip
@inherits StyleComponentBase

    <Styled Name="Chip">

        .Chip-Root {
        color: var(--theme-palette-grey-light-or-dark-contrast-text, rgba(0, 0, 0, 0.87));
        background-color: var(--theme-palette-grey-light-or-dark, #e0e0e0);
        border-radius: 16px;
        padding: 0;
        min-width: 64px;
        height: 32px;
        display: inline-flex;
        outline: 0;
        box-sizing: border-box;
        transition: background-color 300ms cubic-bezier(0.4, 0, 0.2, 1) 0ms,box-shadow 300ms cubic-bezier(0.4, 0, 0.2, 1) 0ms;
        align-items: center;
        font-size: var(--theme-typography-button-font-size, 0.875rem);
        font-family: var(
        --theme-typography-button-font-family,
        "Roboto",
        "Helvetica",
        "Arial",
        sans-serif
        );
        font-weight: var(--theme-typography-button-font-weight, 500);
        line-height: var(--theme-typography-button-line-height, 1.75);
        letter-spacing: var(--theme-typography-button-letter-spacing, 0.02857em);
        white-space: nowrap;
        vertical-align: middle;
        justify-content: center;
        text-decoration: none;
        }

        .Chip-Root:hover {
        text-decoration: none;
        background-color: var(
        --theme-palette-text-primary-hover,
        rgba(0, 0, 0, 0.08)
        );
        }

        .Chip-Root.Chip-Disabled {
        cursor: default;
        pointer-events: none;
        color: var(--theme-palette-action-disabled, rgba(0, 0, 0, 0.26));
        }

        @media (hover: none) {
        .Chip-Root:hover {
        background-color: transparent;
        }
        }

        .Chip-Root:hover.Chip-Disabled {
        background-color: transparent;
        }

        .Chip-Label {
        cursor: inherit;
        display: flex;
        align-items: center;
        user-select: none;
        white-space: nowrap;
        padding-left: 12px;
        padding-right: 12px;
        }

        .Chip-Text {
        @*padding: 6px 8px;*@
        }

        .Chip-Text-Primary {
        color: var(--theme-palette-primary-main, #1976d2);
        }

        .Chip-Text-Primary:hover {
        background-color: var(
        --theme-palette-primary-main-hover,
        rgba(25, 118, 210, 0.08)
        );
        }

        @media (hover: none) {
        .Chip-Text-Primary:hover {
        background-color: transparent;
        }
        }

        .Chip-Text-Secondary {
        color: var(--theme-palette-secondary-main, rgb(220, 0, 78));
        }

        .Chip-Text-Secondary:hover {
        background-color: var(
        --theme-palette-secondary-main-hover,
        var(--theme-palette-secondary-main-hover, rgba(220, 0, 78, 0.08))
        );
        }

        @media (hover: none) {
        .Chip-Text-Secondary:hover {
        background-color: transparent;
        }
        }

        .Chip-Outlined {
        border: 1px solid var(--theme-palette-border-outlined, rgba(0, 0, 0, 0.23));
        padding: 5px 15px;
        }

        .Chip-Outlined.Chip-Disabled {
        border: 1px solid var(--theme-palette-action-disabled, rgba(0, 0, 0, 0.26));

        }

        .Chip-Outlined-Primary {
        color: var(--theme-palette-primary-main, #1976d2);
        border: 1px solid
        var(--theme-palette-primary-main-border, rgba(25, 118, 210, 0.5));
        }

        .Chip-Outlined-Primary:hover {
        border: 1px solid var(--theme-palette-primary-main, #1976d2);
        background-color: var(
        --theme-palette-primary-main-hover,
        rgba(25, 118, 210, 0.08)
        );
        }

        @media (hover: none) {
        .Chip-Outlined-Primary:hover {
        background-color: transparent;
        }
        }

        .Chip-Outlined-Secondary {
        color: var(--theme-palette-secondary-main, rgb(220, 0, 78));
        border: 1px solid
        var(--theme-palette-secondary-main-border, rgba(220, 0, 78, 0.5));
        }

        .Chip-Outlined-Secondary:hover {
        border: 1px solid var(--theme-palette-secondary-main, rgb(220, 0, 78));
        background-color: var(
        --theme-palette-secondary-main-hover,
        rgba(220, 0, 78, 0.08)
        );
        }

        .Chip-Outlined-Secondary.Chip-Disabled {
        border: 1px solid var(--theme-palette-action-disabled, rgba(0, 0, 0, 0.26));

        }

        @media (hover: none) {
        .Chip-Outlined-Secondary:hover {
        background-color: transparent;
        }
        }

        .Chip-Contained {
        color: var(--theme-palette-grey300-contrast-text, rgba(0, 0, 0, 0.87));
        box-shadow: var(
        --theme-shadow2,
        0px 3px 1px -2px rgba(0, 0, 0, 0.2),
        0px 2px 2px 0px rgba(0, 0, 0, 0.14),
        0px 1px 5px 0px rgba(0, 0, 0, 0.12)
        );
        background-color: var(--theme-palette-grey300, #e0e0e0);
        }

        .Chip-Contained:hover {
        box-shadow: var(
        --theme-shadow4,
        0px 2px 4px -1px rgba(0, 0, 0, 0.2),
        0px 4px 5px 0px rgba(0, 0, 0, 0.14),
        0px 1px 10px 0px rgba(0, 0, 0, 0.12)
        );
        background-color: var(--theme-palette-grey-A100, #d5d5d5);
        }

        .Chip-Contained.Chip-FocusVisible {
        box-shadow: var(
        --theme-shadow6,
        0px 3px 5px -1px rgba(0, 0, 0, 0.2),
        0px 6px 10px 0px rgba(0, 0, 0, 0.14),
        0px 1px 18px 0px rgba(0, 0, 0, 0.12)
        );
        }

        .Chip-Contained:active {
        box-shadow: var(
        --theme-shadow8,
        0px 5px 5px -3px rgba(0, 0, 0, 0.2),
        0px 8px 10px 1px rgba(0, 0, 0, 0.14),
        0px 3px 14px 2px rgba(0, 0, 0, 0.12)
        );
        }

        .Chip-Contained.Chip-Disabled {
        color: var(--theme-palette-action-disabled, rgba(0, 0, 0, 0.26));
        box-shadow: none;
        background-color: var(
        --theme-palette-action-disabled-background,
        rgba(0, 0, 0, 0.12)
        );

        }

        @media (hover: none) {
        .Chip-Contained:hover {
        box-shadow: var(
        --theme-shadow2,
        0px 3px 1px -2px rgba(0, 0, 0, 0.2),
        0px 2px 2px 0px rgba(0, 0, 0, 0.14),
        0px 1px 5px 0px rgba(0, 0, 0, 0.12)
        );
        background-color: #e0e0e0;
        }
        }

        .Chip-Contained:hover.Chip-Disabled {
        background-color: var(
        --theme-palette-action-disabled-background,
        rgba(0, 0, 0, 0.12)
        );

        }

        .Chip-Contained-Primary {
        color: var(--theme-palette-primary-contrast-text, #fff);
        background-color: var(--theme-palette-primary-main, #1976d2);
        }

        .Chip-Contained-Primary:hover {
        background-color: var(--theme-palette-primary-current, rgb(17, 82, 147));
        }

        @media (hover: none) {
        .Chip-Contained-Primary:hover {
        background-color: var(--theme-palette-primary-main, #1976d2);
        }
        }

        .Chip-Contained-Secondary {
        color: var(--theme-palette-secondary-contrast-text, #fff);
        background-color: var(--theme-palette-secondary-main, rgb(220, 0, 78));
        }

        .Chip-Contained-Secondary:hover {
        background-color: var(--theme-palette-secondary-dark, rgb(154, 0, 54));
        }

        @media (hover: none) {
        .Chip-Contained-Secondary:hover {
        background-color: var(--theme-palette-secondary-main, rgb(220, 0, 78));
        }
        }

        .Chip-Color-Inherit {
        color: inherit;
        border-color: currentColor;
        }

        .Chip-Text-Size-Small {
        padding: 4px 5px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 13rem);
        }

        .Chip-Text-Size-Large {
        padding: 8px 11px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 15rem);
        }

        .Chip-Outlined-Size-Small {
        padding: 3px 9px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 13rem);
        }

        .Chip-Outlined-Size-Large {
        padding: 7px 21px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 15rem);
        }

        .Chip-Contained-Size-Small {
        padding: 4px 10px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 13rem);
        }

        .Chip-Contained-Size-Large {
        padding: 8px 22px;
        font-size: calc(var(--theme-font-size-rem-factor, 0.0625) * 15rem);
        }

        .Chip-FullWidth {
        width: 100%;
        }

        .Chip-StartIcon {
        display: inherit;
        margin-left: -4px;
        margin-right: 8px;
        }

        .Chip-EndIcon {
        display: inherit;
        margin-left: 8px;
        margin-right: -4px;
        }

        .Chip-Icon-Size-Small > *:first-child {
        font-size: 18px;
        }

        .Chip-Icon-Size-Medium > *:first-child {
        font-size: 20px;
        }

        .Chip-Icon-Size-Large > *:first-child {
        font-size: 22px;
        }

    </Styled>